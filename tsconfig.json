{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "webworker"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmitHelpers": false,
    "importHelpers": true,
    "noResolve": false,
    "noEmit": true,
    "inlineSourceMap": false,
    "declaration": true,
    "outDir": "cjs",
    "declarationDir": "./",
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "cjs"
  ],
  "include": [
    "lib"
  ]
}